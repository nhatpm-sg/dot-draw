# Production environment for Vercel deployment

# WebSocket server URL for collaboration (you can use Railway or your own server)
VITE_APP_WS_SERVER_URL=https://dot-draw-websocket.onrender.com

# Firebase configuration (leave empty to disable Firebase)
VITE_APP_FIREBASE_CONFIG={}

# Production optimizations
VITE_APP_DISABLE_PREVENT_UNLOAD=false
VITE_APP_DISABLE_SENTRY=false
VITE_APP_ENABLE_TRACKING=true

# Library URLs
VITE_APP_LIBRARY_URL=https://libraries.excalidraw.com
VITE_APP_LIBRARY_BACKEND=

# Optional: Backend URLs for persistence (can be empty)
VITE_APP_BACKEND_V2_GET_URL=
VITE_APP_BACKEND_V2_POST_URL=

# Optional: Sentry configuration for error tracking
VITE_APP_SENTRY_DSN=
VITE_APP_GA_TRACKING_ID=
