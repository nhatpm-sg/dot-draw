{
  "buildCommand": "yarn build",
  "outputDirectory": "excalidraw-app/build",
  "installCommand": "yarn install",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],

  "env": {
    "VITE_APP_ENABLE_TRACKING": "true",
    "VITE_APP_WS_SERVER_URL": ""
  }
} 